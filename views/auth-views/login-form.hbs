{{> flash_messages}}

<h2>Log In</h2>

<form action="/process-login" method="POST">

  <label>
    Email:
    <input type="email" name="email" placeholder="quira@solo.com" required="true" />
  </label>

  <label>
    Password:
    <input type="password" name="originalPassword" placeholder="It's a secret" required="true">
  </label>
  <button>Log In</button>
</form>